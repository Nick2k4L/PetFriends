{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "jsx": "react-jsx", // For React 17+ with the new JSX transform
    "strict": true,
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ]

  
}
